{
    "name": "rails-touchbar",
    "displayName": "rails-touchbar",
    "description": "",
    "version": "0.0.1",
    "publisher": "kusumandaru",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
      "onLanguage:ruby"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.pullRebase",
                "title": "Pull rebase",
                "icon": "./assets/pull.png"
            },
            {
                "command": "extension.rubocop",
                "title": "Rubocop",
                "icon": "./assets/robocop.png"
            },
            {
                "command": "extension.push",
                "title": "push",
                "icon": "./assets/push.png"
            },
            {
                "command": "extension.correctRubocop",
                "title": "correct",
                "icon": "./assets/broom.png"
            },
            {
                "command": "extension.goToSpec",
                "title": "goToSpec",
                "icon": "./assets/goToSpec.png"
            }
            
        ],
        "menus": {
            "touchBar": [
                {
                    "command": "extension.pullRebase",
                    "group": "editing"
                },
                {
                    "command": "extension.rubocop",
                    "group": "editing"
                },
                {
                    "command": "extension.push",
                    "group": "editing"
                },
                {
                    "command": "extension.correctRubocop",
                    "group": "editing"
                },
                {
                    "command": "extension.goToSpec",
                    "group": "editing"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/kusumandaru/rails-touchbar.git"
    }
}
